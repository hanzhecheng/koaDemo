#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_koa-generator@1.1.16@koa-generator/bin/koa" "$@"
  ret=$?
else 
  node  "$basedir/../_koa-generator@1.1.16@koa-generator/bin/koa" "$@"
  ret=$?
fi
exit $ret
